<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parrish.test" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateEntry"
            android:label="@string/title_activity_create_entry" >
        </activity>
        <activity
            android:name=".CreateBattery"
            android:label="@string/title_activity_create_battery" >
        </activity>
        <activity
            android:name=".RemoveEntry"
            android:label="@string/title_activity_remove_entry" >
        </activity>
        <activity
            android:name=".ArchiveBattery"
            android:label="" >
        </activity>
        <activity
            android:name=".ViewStatistics"
            android:label="@string/title_activity_view_statistics" >
        </activity>
    </application>

</manifest>
